{
  "cli": {
    "version": ">= 6.0.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_ACCESS_KEY": "$(EXPO_PUBLIC_ACCESS_KEY)",
        "EXPO_PUBLIC_SECRET_KEY": "$(EXPO_PUBLIC_SECRET_KEY)"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ACCESS_KEY": "$(EXPO_PUBLIC_ACCESS_KEY)",
        "EXPO_PUBLIC_SECRET_KEY": "$(EXPO_PUBLIC_SECRET_KEY)"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    }
  },
  "submit": {
    "production": {}
  }
}
